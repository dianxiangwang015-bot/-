loadstring(game:HttpGet("https://github.com/dianxiangwang015-bot/-/blob/main/114514"))()

local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()

-- GUI创建
local LBLG = Instance.new("ScreenGui")
LBLG.Name = "LBLG"
LBLG.Parent = game.CoreGui
LBLG.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
LBLG.Enabled = true

local LBL = Instance.new("TextLabel")
LBL.Name = "LBL"
LBL.Parent = LBLG
LBL.BackgroundColor3 = Color3.new(1, 1, 1)
LBL.BackgroundTransparency = 1
LBL.BorderColor3 = Color3.new(0, 0, 0)
LBL.Position = UDim2.new(0.75,0,0.01,0)
LBL.Size = UDim2.new(0, 133, 0, 30)
LBL.Font = Enum.Font.GothamSemibold
LBL.Text = "Test"
LBL.TextColor3 = Color3.fromRGB(155, 155, 155)
LBL.TextScaled = true
LBL.TextSize = 14
LBL.TextWrapped = true
LBL.Visible = true

-- FPS与时间刷新
local Heartbeat = game:GetService("RunService").Heartbeat
local Start = tick()
local FrameUpdateTable = {}

Heartbeat:Connect(function()
    local LastIteration = tick()
    for Index = #FrameUpdateTable, 1, -1 do
        FrameUpdateTable[Index + 1] = (FrameUpdateTable[Index] >= LastIteration - 1) and FrameUpdateTable[Index] or nil
    end
    FrameUpdateTable[1] = LastIteration
    local CurrentFPS = (tick() - Start >= 1 and #FrameUpdateTable) or (#FrameUpdateTable / (tick() - Start))
    CurrentFPS = CurrentFPS - CurrentFPS % 1
    LBL.Text = ("标准时间:" .. os.date("%H") .. "时" .. os.date("%M") .. "分" .. os.date("%S"))
end)

-- 反挂机
OrionLib:MakeNotification({
    Name = "正在开启反挂机请稍等...",
    Content = "Akf 开启中...",
    Time = 2
})
local vu = game:GetService("VirtualUser")
game.Players.LocalPlayer.Idled:Connect(function()
    vu:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
end)
OrionLib:MakeNotification({
    Name = "开启成功",
    Content = "AKF",
    Time = 1
})

-- 主窗口
local Window = OrionLib:MakeWindow({
    Name = "脚本名",
    HidePremium = false,
    SaveConfig = true,
    IntroText = "加载脚本",
    ConfigFolder = "文件存放名"
})

-- 主页 Tab
local HomeTab = Window:MakeTab({
    Name = "主页",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
HomeTab:AddParagraph("作者", "创客")
HomeTab:AddParagraph("一个介绍")
HomeTab:AddButton({
    Name = "欢迎使用",
    Callback = function ()
        if setclipboard then
            setclipboard("你的群好了")
            OrionLib:MakeNotification({
                Name = "信息",
                Content = "复制请在粘贴板中查看",
                Time = 2
            })
        end
    end
})

-- 通用 Tab
local CommonTab = Window:MakeTab({
    Name = "通用",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

CommonTab:AddSlider({
    Name = "速度",
    Min = 16,
    Max = 200,
    Default = 16,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "数值",
    Callback = function(Value)
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = Value
        end
    end
})

CommonTab:AddSlider({
    Name = "跳跃高度",
    Min = 50,
    Max = 200,
    Default = 50,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "数值",
    Callback = function(Value)
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.JumpPower = Value
        end
    end
})

CommonTab:AddTextbox({
    Name = "移动速度设置",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
        local num = tonumber(Value)
        local player = game.Players.LocalPlayer
        if num and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = num
            OrionLib:MakeNotification({
                Name = "成功设置速度: "..player.Character.Humanoid.WalkSpeed.." |速度",
                Content = "成功",
                Time = 3
            })
        end
    end
})

CommonTab:AddTextbox({
    Name = "重力设置",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
        local num = tonumber(Value)
        if num then
            workspace.Gravity = num
            OrionLib:MakeNotification({
                Name = "成功设置重力: "..workspace.Gravity.." |重力",
                Content = "成功",
                Time = 3
            })
        end
    end
})

CommonTab:AddTextbox({
    Name = "跳跃高度设置",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
        local num = tonumber(Value)
        local player = game.Players.LocalPlayer
        if num and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.JumpPower = num
            OrionLib:MakeNotification({
                Name = "成功设置跳跃高度: "..player.Character.Humanoid.JumpPower.." |高度",
                Content = "成功",
                Time = 3
            })
        end
    end
})

CommonTab:AddSection({ Name = "以下为通用功能" })

CommonTab:AddToggle({
    Name = "夜视",
    Default = false,
    Callback = function(Value)
        if Value then
            game.Lighting.Ambient = Color3.new(1, 1, 1)
        else
            game.Lighting.Ambient = Color3.new(0, 0, 0)
        end
    end
})

-- 穿墙功能
local Noclip = false
local SteppedConn
CommonTab:AddToggle({
    Name = "穿墙",
    Default = false,
    Callback = function(Value)
        Noclip = Value
        local player = game.Players.LocalPlayer
        if Noclip then
            SteppedConn = game:GetService("RunService").Stepped:Connect(function()
                local character = player.Character
                if character then
                    for _, v in pairs(character:GetChildren()) do
                        if v:IsA("BasePart") then
                            v.CanCollide = false
                        end
                    end
                end
            end)
        else
            if SteppedConn then
                SteppedConn:Disconnect()
                SteppedConn = nil
            end
            local character = player.Character
            if character then
                for _, v in pairs(character:GetChildren()) do
                    if v:IsA("BasePart") then
                        v.CanCollide = true
                    end
                end
            end
        end
    end
})

CommonTab:AddButton({
    Name = "名",
    Callback = function()
        -- 可自定义功能
    end
})